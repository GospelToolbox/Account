{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "linebreak-style": "off",
        "comma-dangle": ["error", "never"],
        "jsx-a11y/label-has-for": [ 2, {
            "components": [ "Label" ],
            "required": {
                "some": [ "nesting", "id" ]
            },
            "allowChildren": false
        }],
        "react/forbid-prop-types": "off"
    }
}